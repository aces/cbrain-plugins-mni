<% @userfile.subject_ids.each do |subject_id| %>
  <fieldset>
    <legend>Subject: <%= subject_id %></legend>
    <%= on_click_ajax_replace({:url  => url_for(:action  => :display, :viewer => "civet_study/study_subject", :base_directory  => subject_id), :replace => "#{subject_id}_files", :before  => "<span class='loading_message'>Loading...</span>"}, {:id => subject_id}) do %>
      <span class="ajax_onclick_show_child small warning">Show files</span><span class="ajax_onclick_hide_child small warning" style="display:none">Hide files</span>
    <% end %>
    <span id="<%= subject_id %>_files">
    </span>
  </fieldset> 
<% end %>

